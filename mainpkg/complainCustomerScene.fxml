         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Label>
      <ComboBox fx:id="complainTypeComboBox" layoutX="316.0" layoutY="60.0" onAction="#complainTypeComboBoxItemOnClick" prefWidth="150.0" promptText="Click here" />
      <Label fx:id="complaiResponseLabel" layoutX="86.0" layoutY="319.0" prefHeight="138.0" prefWidth="540.0">
         <font>
            <Font name="System Bold Italic" size="14.0" />
         </font></Label>
      <Button layoutX="250.0" layoutY="285.0" mnemonicParsing="false" onAction="#submitComplainButtonOnclick" prefHeight="34.0" prefWidth="133.0" text="Submit your complain" />
      <TextArea fx:id="AdditionalComplainTextArea" layoutX="86.0" layoutY="134.0" prefHeight="138.0" prefWidth="430.0" promptText="Enter yout additional complains here" />
      <Label layoutX="86.0" layoutY="106.0" text="Additional complain:">
