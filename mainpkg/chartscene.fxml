<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="586.0" prefWidth="617.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainpkg.ChartsceneController">
   <children>
      <PieChart fx:id="pieChart" layoutX="59.0" layoutY="223.0" prefHeight="292.0" prefWidth="482.0" />
      <Button fx:id="loadPieChartButton" layoutX="163.0" layoutY="515.0" mnemonicParsing="false" onAction="#loadChartButtonOnClick" prefHeight="36.0" prefWidth="292.0" text="Click to See FeedBack ratio from all Customers">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
      <TextField fx:id="feedbackValue" layoutX="456.0" layoutY="117.0" promptText="Enter:" />
      <Label layoutX="25.0" layoutY="28.0" text="How do you rate your customer service experience?">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="25.0" layoutY="116.0" text="Rate your customer service experience out of 100!">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="25.0" layoutY="58.0" text="Good/Bad/Satisfactory/Average">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Label>
      <Button fx:id="submitFeedbackButton" layoutX="237.0" layoutY="176.0" mnemonicParsing="false" onAction="#submitFeedbackButtonOnClick" text="Submit Feedback" />
      <ComboBox fx:id="feedbackTypeComboBox" layoutX="234.0" layoutY="55.0" prefWidth="150.0" promptText="Select here:" />
   </children>
</AnchorPane>
